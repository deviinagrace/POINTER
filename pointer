 void swap(int *i, int *j)
 {
     int t;
     t = *i;
     *i = *j;
     *j = t;
 }
 int main()
 {
     int a, b;
     a = 5;
     b = 10;
     printf("%d %d \n", a, b);
     swap(&a,&b);
     printf("%d %d \n", a, b);
 }
